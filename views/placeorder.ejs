<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="icon" href="pic/09092015162707_Poultries_in_Kuwait.png">
        <link rel="stylesheet" href="public/CSS/bootstrap.min.css">
        <link rel="stylesheet" href="public/CSS/placeorder.css">
        <link rel="stylesheet" href="public/CSS/font-awesome.css">
    </head>
    <body class="container">
        <div class="row">
            <div class="small col-sm-6">
                <label class="form-control form-group">Small Bird</label>
                <input class="form-control form-group" id="smallbird" type="number" name="smNoofBox" placeholder="Number of Box" required>
                <input class="form-control form-group" id="smallweight" type="number" step="any" name="smweight" placeholder="Average Weight" required>
            </div>
            <div class="big col-sm-6">
                <label class="form-control form-group">Big Bird</label>
                <input class="form-control form-group" id="bigbird" type="number" name="bgNoofBox" placeholder="Number of Box">
                <input class="form-control form-group" id="bigweight" type="number" step="any" name="bgweight" placeholder="Average Weight">
            </div>
            <button class="btn btn-block form-group" onclick="placeorder()">Place Order</button>
            <button onclick="goback()" class="btn-primary btn-sm" id="button">Go back</button>
        </div>    
        <script src="public/JS/jquery-3.3.1.min.js"></script>
        <script src="public/JS/bootstrap.min.js"></script>
        <script src="public/JS/placeorder.js"></script>
    </body>
</html>